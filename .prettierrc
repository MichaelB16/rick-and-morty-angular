{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "strict",
  "singleAttributePerLine": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 100,
        "singleAttributePerLine": true,
        "bracketSameLine": false,
        "htmlWhitespaceSensitivity": "strict",
        "parser": "angular",
        "tabWidth": 2
      }
    }
  ]
}